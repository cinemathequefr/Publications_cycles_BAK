          "annee": 1929,
          "pays": "France",
          "duree": 13,
          "version": "",
          "format": "",
          "ageMinimal": null,
          "textes": [
            {
              "typeTexte": 9,
              "texte": "Le Bernard l'ermite est un crustacé marin qui habite les coquilles vides. Pour en choisir une, il l'inspecte, la mesure et la soupèse."
            }
          ]
        },
        {
          "idSeance": 33824,
          "ordre": 6,
          "dateHeure": "2020-03-13T21:30:00",
          "idSalle": [
